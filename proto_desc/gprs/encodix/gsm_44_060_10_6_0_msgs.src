slave <Dynamic Allocation struct > ::=
   < EXTENDED_DYNAMIC_ ALLOCATION : bit (1) >
   { 0 | 1 < P0 : bit (4) >
            < PR_MODE : bit (1) > }
   < USF_GRANULARITY : bit (1) >
   0                       -- The value '1' was allocated in an earlier version of the protocol and shall not be used.
   { 0 | 1 < TBF Starting Time : < Starting Frame Number Description IE > > }
   {0                      -- Timeslot Allocation
        { 0 | 1 < USF_TN0 : bit (3) > }
        { 0 | 1 < USF_TN1 : bit (3) > }
        { 0 | 1 < USF_TN2 : bit (3) > }
        { 0 | 1 < USF_TN3 : bit (3) > }
        { 0 | 1 < USF_TN4 : bit (3) > }
        { 0 | 1 < USF_TN5 : bit (3) > }
        { 0 | 1 < USF_TN6 : bit (3) > }
        { 0 | 1 < USF_TN7 : bit (3) > }
   |1                      -- Timeslot Allocation with Power Control Parameters
        < ALPHA : bit (4) >
        { 0 | 1 < USF_TN0 : bit (3) >
                 < GAMMA_TN0 : bit (5) > }
        { 0 | 1 < USF_TN1 : bit (3) >
                 < GAMMA_TN1 : bit (5) > }
        { 0 | 1 < USF_TN2 : bit (3) >
                 < GAMMA_TN2 : bit (5) > }
        { 0 | 1 < USF_TN3 : bit (3) >
                 < GAMMA_TN3 : bit (5) > }
        { 0 | 1 < USF_TN4 : bit (3) >
                 < GAMMA_TN4 : bit (5) > }
        { 0 | 1 < USF_TN5 : bit (3) >
                 < GAMMA_TN5 : bit (5) > }
        { 0 | 1 < USF_TN6 : bit (3) >
                 < GAMMA_TN6 : bit (5) > }
        { 0 | 1 < USF_TN7 : bit (3) >
                 < GAMMA_TN7 : bit (5) > } } ;

slave < Dynamic Allocation 2 struct > ::=
   < EXTENDED_DYNAMIC_ALLOCATION : bit (1) >
   { 0 | 1 < P0_C1 : bit (4) >
            < PR_MODE_C1 : bit (1) >
            { 0 | 1 < P0_C2 : bit (4) >
                      < PR_MODE_C2 : bit (1) > } }
   < USF_GRANULARITY : bit (1) >
   { 0 | 1 < UPLINK_TFI_ASSIGNMENT : bit (5) > }
   {0            -- Allocation without Power Control Parameters
        < N_USF : bit (4) >
        { 0 | 1 < USF : bit (3) > } *( val(N_USF) + 1)
   |1            -- Allocation with Power Control Parameters
        < ALPHA_C1: bit (4) >
        { 0 | 1 < ALPHA_C2: bit (4) > }
        {0                           -- BTTI mode
            < N_TS : bit (4) >
            {0|1
                 < USF : bit (3) >
                 < GAMMA: bit (5) >
            } * ( val(N_TS) + 1)
        |1                           -- RTTI mode
            < N_PAIRS : bit (3) >
            {0|1
                 < USF : bit (3) >
                 < GAMMA : bit (5) >
            } * (val(N_PAIRS) + 1)
            {0        -- RTTI USF
            |1        -- BTTI USF
                 { 0 | 1 < USF_2 : bit (3) >
                      { 0 | 1 < GAMMA : bit (5) > }
                 } * (val(N_PAIRS) + 1)
            }
        }
   };

