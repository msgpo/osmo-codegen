            < DOWNLINK_PDCH_PAIRS_C2 : bit (8) >
            < UPLINK_PDCH_PAIRS_C1 : bit (8) >
            < UPLINK_PDCH_PAIRS_C2 : bit (8) >
       ! < PDCH pairs configuration error : { 1 1 } bit (*) = < no string > >   -- reserved
       }
       < RTTI_DOWNLINK_PDCH_PAIR_ASSIGNMENT_DC : bit (8) >
   }
